<div class="page-header">
  <h1>
    Voting boards
    <a href="/voting_boards/new" class="btn btn-lg btn-success">New Voting board</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Venue</th>
        <th>Event</th>
        <th>Vote</th>
        <th>Actions</th>
      </tr>

      <% @voting_boards.each do |voting_board| %>
      <tr>
        <td><%= voting_board.venue_id %></td>
        <td><%= voting_board.event_id %></td>
        <td><%= voting_board.vote %></td>
        <td>
          <a href="/voting_boards/<%= voting_board.id %>" class="btn btn-primary">Show</a>
          <a href="/voting_boards/<%= voting_board.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_voting_board/<%= voting_board.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
